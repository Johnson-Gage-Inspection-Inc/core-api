# .env.example
# Copy this file to .env and fill in your actual secrets

# Qualer API Integration
QUALER_API_KEY=your-qualer-api-key-here

# Azure Authentication
AZURE_CLIENT_SECRET=your-azure-client-secret-here

# Database Connection
DATABASE_URL=postgresql+psycopg2://username:password@host:5432/pyro

# Development Settings (optional)
SKIP_AUTH=false

# Production Only (if deploying)
DEPLOY_TOKEN=your-deploy-token-here
